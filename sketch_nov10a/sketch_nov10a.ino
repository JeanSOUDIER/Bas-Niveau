  for(int l=0;l<512;l++) {
    Write(0);
    Enable();
    if(l == 64 || l == 128 || l == 192 || l == 256 || l == 320 || l == 384 || l == 448) {
      digitalWrite(DI, 0);
      delay(10);
      Write(184+(int)(l/64));

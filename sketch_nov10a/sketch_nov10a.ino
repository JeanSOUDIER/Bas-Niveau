      Write(64);
      Enable();
      digitalWrite(DI, 10);
      delay(1);
    }
  }
  digitalWrite(DI, 0);
  delay(10);
  digitalWrite(CS1, 1);
  digitalWrite(CS2, 0);
  Write(184);
  Enable();
  Write(119);
  Enable();
  digitalWrite(DI, 1);
  delay(10);
  for(int l=0;l<4096;l++) {
    Write(0);
    Enable();
    if(l == 64 || l == 128 || l == 192 || l == 256 || l == 320 || l == 384 || l == 448) {
      digitalWrite(DI, 0);
      delay(10);
      Write(184+(int)(l/64));
      Enable();
      Write(64);
      Enable();
      digitalWrite(DI, 1);
      delay(10);
    }
  }
